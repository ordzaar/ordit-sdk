{
  "name": "@ordzaar/ordit-sdk",
  "version": "0.0.0",
  "private": true,
  "repository": "https://github.com/ordzaar/ordit-sdk",
  "bugs": "https://github.com/ordzaar/ordit-sdk/issues",
  "scripts": {
    "prepare": "husky install",
    "build": "turbo run build",
    "clean": "turbo run clean",
    "format": "prettier --write .",
    "lint": "turbo run lint -- --fix",
    "test": "turbo run test",
    "dev": "turbo run dev",
    "docs:dev": "turbo run dev --filter=docs",
    "docs:build": "turbo run build --filter=docs",
    "docs:clean": "turbo run clean --filter=docs"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.4.8",
    "@changesets/cli": "2.26.2",
    "@stickyjs/prettier": "^1.3.4",
    "@stickyjs/turbo": "^1.3.4",
    "@vitest/coverage-v8": "^0.34.6",
    "@waveshq/standard-prettier": "^2.20.0",
    "husky": "^8.0.3",
    "rimraf": "^5.0.5",
    "semver": "^7.5.4",
    "typescript": "5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.9.2"
  }
}
